	"license": "MIT",
	"private": true,
	"scripts": {
		"format": "prettier \"**/*.+(js|jsx|json|yml|yaml|html|css|less|scss|ts|tsx|md|mdx|graphql|vue|py|java|php|sol)\" --write",
