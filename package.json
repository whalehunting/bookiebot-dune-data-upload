		"@graphql-codegen/typescript-operations": "^4.0.1",
		"@json2csv/plainjs": "^7.0.3",
		"@types/node": "^20.8.3",
		"@typescript-eslint/eslint-plugin": "^6.7.4",
		"@typescript-eslint/parser": "^6.7.4",
		"eslint": "^8.51.0",
		"graphql": "^16.8.1",
		"graphql-request": "^6.1.0",
		"prettier": "^3.0.3",
		"ts-node": "^10.9.1",
		"typechain": "^8.3.1",
		"typescript": "^5.2.2"
	},
	"packageManager": "yarn@3.6.4"
}
